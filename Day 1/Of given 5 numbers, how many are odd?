#include<stdio.h>
#include<string.h>
// Read only region start

int countEvens(int input1,int input2,int input3,int input4,int input5)
{
    // Read only region end
	// Write code here
    int count_odd=0;
    if(input1%2!=0)
        count_odd++;
        else if(input2%2!=0)
            count_odd++;
            else if(input3%2!=0)
                count_odd++;
                 else if(input4%2!=0)
                     count_odd++;
                     else if(input5%2!=0)
                         count_odd++;
    return count_odd;

}
